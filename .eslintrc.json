{
  "plugins": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:@typescript-eslint/recommended"],
  "env": {
    "es2023": true,
    "browser": false,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn"
  },
  "overrides": [
  ]
}
